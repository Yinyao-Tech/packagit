{
  "name": "packagit/packagit",
  "description": "amazing laravel module management with hacking tricks",
  "keywords": ["packagit", "laravel modules", "artisan"],
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Yin Qisen",
      "email": "yinqisen@gmail.com"
    }
  ],
  "bin": [
    "bin/packagit",
    "bin/p"
  ],
  "autoload": {
    "psr-4": {
      "Packagit\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Packagit\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
